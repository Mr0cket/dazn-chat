{
  "name": "Dazn Chat",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Adds a global chat to the DAZN website - chat and banter with other fans watching the same live event!",
  "permissions": ["storage", "tabs", "activeTab"],
  "background": {
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_title": "DAZN-Chat"
  },
  "web_accessible_resources": [
    "chatUi/index.html",
    "chatUi/test.html",
    "chatUi/jquery.min.js",
    "chatUi/socket.io.js",
    "chatUi/index.js",
    "chatUi/socket.io.js.map"
  ],
  "icons": {
    "48": "assets/icons/icon48.png",
    "200": "assets/icons/icon.png"
  }
}
